{
	"device": "LG G2 Mini",
	"target architecture": "armv7a",
	"supports neon floating point unit": "true",
	"mixer settings":
	{
		"mixer paths xml file": "/system/etc/mixer_paths.xml",
		"mixer volumes xml file": "",
		"playback path names":
		{ 
			"line-out": "headphones-lite",
			"speaker": "speaker",
			"handset": "handset"
		},
		"capture path names":
		{
			"line-in": "lg-loopback-headset-mic",
		   	"mic": "lg-loopback-earpiece-mic",
		   	"handset": "handset-mic"
		},
		"default playback device number": 15,
		"default capture device number": 0,
		"mixer playback device activation": "SLIMBUS_0_RX Audio Mixer DEVICE_ID",
		"mixer capture device activation": "DEVICE_ID Mixer SLIM_0_TX",
		"mixer playback device secondary activation": "",
		"mixer capture device secondary activation": ""
	},
	"control outputs":
	{
		"flashlight":
		{
			"control file": "/sys/class/leds/led:flash_torch/brightness",
			"max value": "/sys/class/leds/led:flash_torch/max_brightness"
		},
		"lcd-backlight":
		{
			"control file": "/sys/class/leds/lcd-backlight/brightness",
			"max value": "/sys/class/leds/lcd-backlight/max_brightness"
		}, 
		"vibration":
		{ 
			"control file": "/sys/class/timed_output/vibrator/enable"
		}
	}
}
